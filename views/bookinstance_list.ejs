<% layout('layout') -%>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Book</th>
      <th scope="col">status</th>
      <th scope="col">author</th>
    </tr>
  </thead>
  <tbody>
    <% bookinstance_list.forEach(function(bookinstance) { %>
      <tr onclick="window.location='<%= book.url %>';">
         <td><%= bookinstance.book.title %></td>
         <td><%= bookinstance.status %></td>
       </tr>
    <% }); %>
  </tbody>
</table>
